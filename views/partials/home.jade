script#home(type="text/ng-template")
    div(class = "jumbotron mainJumbo")
        div(class = "container")
            h1.
                {{welcomeMessage}}
            p.
                Enter your Realm and Guild name to see your raid progression!
    div(class = "well")
        div.container
            h2.text-center
             | Get Started
            div.container
                div(class = "requiredInfo").container
                    form(role="form")
                        div(class = "form-group")
                            label(for="realmName") Realm
                            input(type = "input", class = "form-control", id ="iRealmName", ng-model = "realm", placeholder = "Enter Realm Name")
                        div(class = "form-group")
                            label(for="guildName") Guild
                            input(type = "input", class = "form-control", id ="iRealmName", ng-model = "guild", placeholder = "Enter Guild Name")
                        div(class = "form-group")
                            label(for="characterName") Character
                            input(type = "input", class = "form-control", id ="iRealmName", ng-model = "characterName", placeholder = "Enter Character Name")
                        button(type = "button", class = "btn btn-primary", ng-click = "getCharacter()")
                           | Get Info
    div(class = "well")
        div().container
            div()
                h1.text-center
                    | Guild Name Here
            div.container.text-center
                div.row
                    div.col-md-6
                        h3
                            | Realm
                            br
                            small
                                | someRealm
                    div.col-md-6
                        h3
                            | Members
                            br
                            small
                                | #ofMembers
    div(ng-class = "classColor").well
        div.container
            img(ng-src = "{{characterImage}}").fixedPosition
            div()
                h1.text-center
                    {{characterName}}
            div.container
                div.row
                    div.col-md-4.text-center
                        h3
                            | iLevel
                            br
                            small
                                {{iLvl}}
                    div.col-md-4.text-center
                        h3
                            | Acheivment Points
                            br
                            small
                                {{achievmentPoints}}
                    div.col-md-4.text-center
                        h3
                            | Specs
                            br
                            small
                                | Resto & Elemental
